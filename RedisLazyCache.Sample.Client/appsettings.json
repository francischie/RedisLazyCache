{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override" : {
        "Microsoft": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss} {Level:u3}] {Message}{NewLine}{Exception}"
        }
      }
    ]
  },
  "ConnectionStrings": {
    "Redis": "localhost:6379,connectTimeout=1000,abortConnect=false"
  }
}
